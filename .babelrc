{
    "presets": ["es2015", "react"],
    "plugins": ["transform-object-rest-spread", "syntax-object-rest-spread"],
    "env": {
        "development": {
            "plugins": [
                ["react-transform", {
                    "transforms": [{
                        "transform": "react-transform-hmr",
                        "imports": ["react"],
                        "locals": ["module"]
                    }]
                }]
            ]
        }
    }
}